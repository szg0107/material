# 主要用于配置IDE
root = true
# 对所有文件都有效  [*js]只对js文件有效
[*]
# 设置编码格式
charset = utf-8
# 缩进类型 可选 space 和 tab
indent_style = space
# 缩进数量可选 2 or 4，或者tab
indent_size = 2
# 换行符格式
end_of_line = lf
# 是否在文件的最后插入一个空行 可选 true和 false
insert_final_newline = true
# 是否删除尾行空格 可选择 true 和 false
trim_trailing_whitespace = true
